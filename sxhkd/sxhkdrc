super + Return
	alacritty

super + t
	telegram-desktop
	
super + shift + {q,r}
    bspc {quit,wm -r}

super + f
	firefox

super + e
    nemo
    
super + g
    geany

super + q 
	bspc node -{c}

super + shift + {t,s,f}
	bspc node -t {tiled,floating,fullscreen}

super + {_,shift + }{Left,Down,Up,Right}
    bspc node -{f,s} {west,south,north,east}

super + {_,shift + }{1-5,0}
  bspc {desktop -f, node -d} '^{1-5,10}' --follow

super + ctrl + {Left,Down,Up,Right}
	bspc node -z {right -80 0,top 0 80,bottom 0 -80,left 80 0}

ctrl + {Left,Down,Up,Right}
	bspc node -z {left -80 0,bottom 0 80,top 0 -80,right 80 0}

super + d
    rofi -show drun -modi drun -show-icons

super + l
    betterlockscreen -l

super + {1,2,3,4,5}
	bspc desktop -f {1,2,3,4,5}

super + m
	bspc desktop -l next

super + p
    rofi -show power-menu -modi power-menu:rofi-power-menu

super + Print
    flameshot gui

XF86AudioRaiseVolume
	amixer set Master 5%+

XF86AudioLowerVolume
	amixer set Master 5%-

XF86AudioMute
    amixer set Master toggle

XF86MonBrightnessUp 
    brightnessctl set +5%

XF86MonBrightnessDown 
    brightnessctl set 5%-
    
